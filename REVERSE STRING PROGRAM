using System;

class Program {
    static void Main(string[] args) {
        string Target = "Target Systems";
        Console.WriteLine("Original string: " + Target);

        char[] reversed = new char[Target.Length];
        for (int i = 0; i < Target.Length; i++) {
            reversed[i] = Target[Target.Length - i - 1];
        }

        string result = new string(reversed);
        Console.WriteLine("Reversed string: " + result);
    }
}
